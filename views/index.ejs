<!DOCTYPE html>
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="apple-mobile-web-app-status-bar-style" content="black-translucent" />
		<meta name="viewport" content="initial-scale=1.0,maximum-scale=1.0" />

		<title>Bingo Game</title>

		<link href="css/jquery.mobile-1.1.1.min.css" rel="stylesheet" type="text/css">
		<script type="text/javascript" src="js/jquery.min.js"></script>
		<script type="text/javascript" src="js/jquery.mobile-1.1.1.min.js"></script>
		<script type="text/javascript" src="js/viewporter.js"></script>
		<script type="text/javascript" src="js/graphic.js"></script>
		<script src="http://localhost:8080/socket.io/socket.io.js"></script>
		<script src="js/game.js"></script>
	</head>

	<body>
		<div id="roomPage" data-role="page">

			<div data-role="header" data-theme="e" data-position="fixed">
				<h1>Bingo! Rooms <span id="onlineUserCount"></span></h1>
			</div><!-- /header -->

			<div data-role="content">	
				<ul id="roomList" data-role="listview">					
				</ul>
			</div><!-- /content -->
			<div data-role="footer" data-theme="e" data-position="fixed">
				<h4>jstanbul.org</h4>
			</div><!-- /footer -->
			
		</div><!-- /page -->
		<!-- Start of second page -->
		<div id="gamePage" data-role="page">

			<div data-role="header" data-theme="e" data-position="fixed">
				<a href="#" class="backToRoomPage" data-icon="back" class="ui-btn-left">Back</a>
				<span class="ui-title" />
			</div><!-- /header -->

			<div data-role="content">
				<h3><span id="gameFinishedSpan"><span></h3>
				<br/>	
				<h3>Chosen Number:<span id="chosenNumberSpan"></span></h3>
				<br/>
				<div class="ui-grid-a">
					<div class="ui-block-a">
						<div class="ui-grid-c">
							<div class="ui-block-a 1_Column"></div>
							<div class="ui-block-b 2_Column"></div>
							<div class="ui-block-c 3_Column"></div>
							<div class="ui-block-d 4_Column"></div>
						</div><!-- /grid-c -->
					</div>
					<div class="ui-block-b">
						<div class="ui-grid-d">
							<div class="ui-block-a 5_Column"></div>
							<div class="ui-block-b 6_Column"></div>
							<div class="ui-block-c 7_Column"></div>
							<div class="ui-block-d 8_Column"></div>
							<div class="ui-block-e 9_Column"></div>
						</div><!-- /grid-c -->
					</div>
				</div><!-- /grid-a -->
			</div><!-- /content -->

			<div data-role="footer" data-theme="e" data-position="fixed">
				<h4>jstanbul.org</h4>
			</div><!-- /footer -->
		</div><!-- /page -->
	</body>
</html>